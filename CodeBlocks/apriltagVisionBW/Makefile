$(OBJ)/cameraCapture.o: $(SRC)/cameraCapture.c
	$(CC) $(CFLAGS) $(INC) -c $(SRC)/cameraCapture.c -o $(OBJ)/cameraCapture.o

all: $(COBJ)/apriltagVision.o $(COBJ)/opencv.o $(COBJ)/ProcessApriltags.o $(COBJ)/ProcessApriltagsFast.o $(OBJ)/cameraCapture.o
